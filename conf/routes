GET     /                                   controllers.UIController.index
GET     /login                              controllers.UIController.login
GET     /projects                           controllers.UIController.list
GET     /projectsSetup                      controllers.UIController.setup
GET     /project_details                    controllers.UIController.details
GET     /profile                            controllers.UIController.profile
GET     /new_project                        controllers.UIController.newProject
PUT     /new_project                        controllers.UIController.CreateNewProject
GET     /user_project                       controllers.UIController.usersProject
GET     /issues                             controllers.UIController.issues
GET     /user_management                    controllers.UIController.management
GET     /getAllGitHubProject/:username      controllers.UIController.getAllGitHubProjects(username: String)
GET     /getAllToggleProject/:username      controllers.UIController.getAllToggleProjects(username: String)

GET     /toggl                              controllers.TEAHubController.togglProjects
GET     /assets/*file                       controllers.Assets.at(path = "/public", file)
